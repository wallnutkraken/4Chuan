        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Jackie4Chuan"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Image" Height="300" Width="300">
    <Image x:Name="image" MouseLeftButtonUp="image_MouseUp" MouseRightButtonUp="image_MouseRightButtonUp" HorizontalAlignment="Center" Height="1" VerticalAlignment="Center" Width="1"/>

